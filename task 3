global  _start	
Z equ 48
N equ 7
L equ 8
section .bss						

var_d:       resb L
section .text					
_start:
    mov al, 127
    mov ecx, 0
    L1:
    mov bl, Z
    mov [var_d+ecx], bl
    
    shr al, 1
    
    setl bl
    add [var_d+ecx], bl
    Loop L1
    
    
    
    mov rax, 1
    mov rdi, 1
    mov rsi, var_d
    mov rdx, L
    syscall
    
     mov rax, 60
    mov rdi, 0 
    syscall
    ret
