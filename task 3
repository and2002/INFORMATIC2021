global  _start						
section .bss						

var_d:       resb 1
section .text					
_start:
    mov bl, 48
    mov [var_d], bl
    
    mov al, 200
    mov bl, al
    shr al, 1
    
    cmp bx, [al]
    JE L1 ;если был 0
    
    cmp bx, [al]
    JG L2 ;если был 1
    
    jmp L3
    
    L1:
    
    mov bl, 0
    mov [var_d], bl
    
    jmp L3
    
    L2:
    mov bl, 1
    mov [var_d], bl
    
    jmp L3
    
    L3:
    mov rax, 1
    mov rdi, 1
    mov rsi, var_d
    mov rdx, 1
    syscall
    
    mov rax, 60
    mov rdi, 0 
    syscall
    ret
